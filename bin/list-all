#!/usr/bin/env bash

source "$(dirname "$0")/../lib/utils.sh"

list_all() {
  local platform=$(platform)
  local archname=$(archname)
  http_get 'https://raw.githubusercontent.com/skaji/rakudo-releases/main/rakudo-releases.v1.csv' | \
    grep ,archive, | \
    grep ,moar, | \
    grep ",$platform," | \
    grep ",$archname," | \
    cut -d, -f11 | \
    sort | \
    tr '\n' ' '
}

list_all
